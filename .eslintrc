{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "jest": true,
        "browser": true
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/forbid-prop-types": [0],
        "import/no-named-as-default": [0],
        "import/no-unresolved": [0],
        "no-console": [0]
    }
}